{
  "name": "fridge",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && node server.js",
    "build": "cd frontend && npm run build",
    "start": "cd backend && node server.js"
  },
  "dependencies": {
    "concurrently": "^7.6.0"
  }
}
